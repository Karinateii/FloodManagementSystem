@model NewLagosFloodDetectionSystem.Models.City
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

}
<style>
    body {
        background-color: white;
    }
</style>

<dl class="text-dark ml-5">
    <dt>@Html.DisplayNameFor(model => model.Name)</dt>
    <dd>@Html.DisplayFor(model => model.Name)</dd>
    <dt>@Html.DisplayNameFor(model => model.Predictions)</dt>
    <dd class="col-sm-10">
        <table class="table">
            <tr>
                <th>
                    Year
                </th>
                <th>
                    Month
                </th>
                <th>
                    Prediction
                </th>
            </tr>
            @foreach (var item in Model.Predictions)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item.Year)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.Month)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.Prediction)
                    </td>
                </tr>
            }
        </table>
    </dd>
</dl>

